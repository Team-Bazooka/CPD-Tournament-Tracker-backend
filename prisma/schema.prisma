generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model member {
  id               Int      @id @default(autoincrement())
  fname            String
  lname            String
  student_id       String   @unique
  team_id          Int
  kattis_acct_link String
  tg_username      String   @default("")
  email            String
  password         String
  role             String
  registered_at    DateTime
}

model team {
  id         Int      @id @default(autoincrement())
  name       String
  logo_url   String
  created_At DateTime
}
